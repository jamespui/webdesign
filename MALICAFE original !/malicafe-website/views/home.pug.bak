doctype html
html(lang='en')
    head
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        title #{title}
        meta(name='description', content='Free Bootstrap Theme by uicookies.com')
        meta(name='keywords', content='free website templates, free bootstrap themes, free template, free bootstrap, free website template')
        link(href='https://fonts.googleapis.com/css?family=Open+Sans:400,700|Pinyon+Script', rel='stylesheet')
        link(rel='stylesheet', href='../static/resto/css/styles-merged.css')
        link(rel='stylesheet', href='../static/resto/css/style.min.css')
        //if lt IE 9
          script(src='js/vendor/html5shiv.min.js')
          script(src='js/vendor/respond.min.js')
    body
        // Fixed navbar
        nav.navbar.navbar-default.navbar-fixed-top.probootstrap-navbar
            .container
                .navbar-header
                    button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar-collapse', aria-expanded='false', aria-controls='navbar')
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    a.navbar-brand(href='/', title='#{title}') #{title}
                #navbar-collapse.navbar-collapse.collapse
                    ul.nav.navbar-nav.navbar-right
                        li.active
                            a(href='#', data-nav-section='welcome') Welcome
                        li
                            a(href='#', data-nav-section='specialties') Specialties
                        li
                            a(href='#', data-nav-section='menu') Menu
                        li
                            a(href='#', data-nav-section='reservation') Reservation
                        li
                            a(href='#', data-nav-section='events') Events
                        li
                            a(href='#', data-nav-section='contact') Contact
