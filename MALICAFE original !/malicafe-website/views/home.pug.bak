        section.probootstrap-section.probootstrap-bg-white
            .container
                .row
                    .col-md-12.probootstrap-animate
                        form.probootstrap-form(method='post')
                            .row
                                .col-md-4
                                    .form-group
                                        label(for='people') How Many People
                                        .form-field
                                            i.icon.icon-chevron-down
                                            select#people.form-control(name='people')
                                                option(value='#') 1 people
                                                option(value='#') 2 people
                                                option(value='#') 3 people
                                                option(value='#') 4+ people
                                .col-md-4
                                    .form-group
                                        label(for='date') Date
                                        .form-field
                                            i.icon.icon-calendar
                                            input#date.form-control(type='text')
                                .col-md-4
                                    .form-group
                                        label(for='time') Time
                                        .form-field
                                            i.icon.icon-clock
                                            input#time.form-control(type='text')
                            // END row
                            .row
                                .col-md-4
                                    .form-group
                                        label(for='name') Name
                                        .form-field
                                            i.icon.icon-user2
                                            input#name.form-control(type='text', placeholder='Your full name')
                                .col-md-4
                                    .form-group
                                        label(for='email') Email
                                        .form-field
                                            i.icon.icon-mail
                                            input#email.form-control(type='text', placeholder='Your email address')
                                .col-md-4
                                    .form-group
                                        label(for='phone') Phone
