doctype html
html(lang='en')
    head
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        title #{title}
        meta(name='description', content='Free Bootstrap Theme by uicookies.com')
        meta(name='keywords', content='free website templates, free bootstrap themes, free template, free bootstrap, free website template')
        link(href='https://fonts.googleapis.com/css?family=Open+Sans:400,700|Pinyon+Script', rel='stylesheet')
        link(rel='stylesheet', href='../static/resto/css/styles-merged.css')
        link(rel='stylesheet', href='../static/resto/css/style.min.css')
        //if lt IE 9
          script(src='js/vendor/html5shiv.min.js')
          script(src='js/vendor/respond.min.js')
    body
        // Fixed navbar
        nav.navbar.navbar-default.navbar-fixed-top.probootstrap-navbar
            .container
                .navbar-header
                    button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar-collapse', aria-expanded='false', aria-controls='navbar')
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    a.navbar-brand(href='/', title='#{title}') #{title}
                #navbar-collapse.navbar-collapse.collapse
                    ul.nav.navbar-nav.navbar-right
                        li.active
                            a(href='#', data-nav-section='welcome') home
                        li
                            a(href='#', data-nav-section='specialties')  specialties
                        li
                            a(href='#', data-nav-section='menu') menu
                        li
                            a(href='#', data-nav-section='reservation') location
                        li
                            a(href='#', data-nav-section='events') how to order
                        li
                            a(href='#', data-nav-section='contact') contact
                        li
                            a(href='#', data-nav-section='register') register
                        li
                            a(href='login', data-nav-section='login') login


        section.flexslider(data-section='welcome')
            ul.slides
                li.overlay(style='background-image: url(../static/resto/img/malicafe.jpg)', data-stellar-background-ratio='1.0')
                    .container
                        .row
                            .col-md-8.col-md-offset-2
                                .probootstrap-slider-text.text-center.probootstrap-animate.probootstrap-heading
                                    h1.primary-heading Welcome To
                                    h3.secondary-heading MALICAFE
                                    p.sub-heading
                                        | Introduction

                li.overlay(style='background-image: url(../static/resto/img/kolomee.jpg)')
                    .container
                        .row
                            .col-md-8.col-md-offset-2
                                .probootstrap-slider-text.text-center.probootstrap-animate.probootstrap-heading
                                    h1.primary-heading Dine
                                    h3.secondary-heading With Us
                                    p.sub-heading
                                        | Introduction

                li.overlay(style='background-image: url(../static/resto/img/malicafe2.jpg)')
                    .container
                        .row
                            .col-md-8.col-md-offset-2
                                .probootstrap-slider-text.text-center.probootstrap-animate.probootstrap-heading
                                    h1.primary-heading Enjoy
                                    h3.secondary-heading With Us
                                    p.sub-heading
                                        | Introduction

        section.probootstrap-section.probootstrap-bg-white
            .container
                .row
                    .col-md-5.text-center.probootstrap-animate
                        .probootstrap-heading.dark
                            h1.primary-heading Location
                            h3.secondary-heading Our Store
                            span.seperator * * *
                        p
                            | Introduction ********* *********** *********** ****************** ************ **************** ********
                        p
                            a.probootstrap-custom-link(href='#') About Us
                    .col-md-6.col-md-push-1.probootstrap-animate
                        p
                            img.img-responsive(src='../static/resto/img/malicafe3.jpg', alt='Free Bootstrap Template by uicookies.com')
                // END row
        section.probootstrap-section-bg.overlay(style='background-image: url(../static/resto/img/rojak.jpg);', data-stellar-background-ratio='0.85', data-section='specialties')
            .container
                .row
                    .col-md-12.text-center.probootstrap-animate
                        .probootstrap-heading
                            h2.primary-heading Menu
                            h3.secondary-heading Our Specialties
        // probootstrap-bg-white
        section.probootstrap-section
            .container
                .row
                    .probootstrap-cell-retro
                        .half
                            .probootstrap-cell.probootstrap-animate(data-animate-effect='fadeIn')
                                .image(style='background-image: url(../static/resto/img/kolomee.jpg);')
                                .text.text-center
                                    h3 Kolo Mee
                                    p Calories ************** ******** ********** **************

                            .probootstrap-cell.reverse.probootstrap-animate(data-animate-effect='fadeIn')
