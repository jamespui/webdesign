                                    h3 Mee Jawa
                                    p Calories ************** ******** ******** **************

                            .probootstrap-cell.probootstrap-animate(data-animate-effect='fadeIn')
                                .image(style='background-image: url(../static/resto/img/rojak.jpg);')
                                .text.text-center
                                    h3 Rojak
                                    p Calories ************** ************ ******* **************

                        .half
                            .probootstrap-cell.probootstrap-animate(data-animate-effect='fadeIn')
                                .image(style='background-image: url(../static/resto/img/steamchickenrice.jpg);')
                                .text.text-center
                                    h3 Steamed Chicken Rice
                                    p Calories ************** ********** *********** ********


                            .probootstrap-cell.reverse.probootstrap-animate(data-animate-effect='fadeIn')
